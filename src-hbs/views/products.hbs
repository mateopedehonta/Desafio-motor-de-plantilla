    <h1>Productos</h1>
    <div class="container">
        {{#each productos}}
            <div class="box">
                <h2>{{this.title}}</h2>
                <p>Precio:{{this.price}}</p>
                <img src="{{this.url}}" alt="">
            </div>
        {{/each}}
    </div>

<a href="/">Agregar productos</a>